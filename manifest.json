{
  "manifest_version": 3,
  "name": "Random Bookmark Opener",
  "version": "1.0",
  "description": "Opens a random bookmark from a specified folder",
  "permissions": [
    "bookmarks",
    "tabs",
    "storage"
  ],
  "action": {},
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}